{
  "name": "gatsby-starter-theme-yalc-workspace",
  "private": true,
  "description": "Starter for creating a Gatsby Theme workspace using Yalc.",
  "version": "0.0.0-development",
  "license": "MIT",
  "author": "Danail Minchev <danail.dev@gmail.com>",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "commit": "commit"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@commitlint/prompt-cli": "12.1.1",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.2.1",
    "yalc": "1.0.0-pre.53"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": "prettier --write"
  }
}
