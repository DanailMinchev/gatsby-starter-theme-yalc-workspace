{
  "name": "gatsby-theme-minimal",
  "description": "A simple Gatsby theme",
  "version": "0.0.0-development",
  "main": "index.js",
  "license": "MIT",
  "author": "Danail Minchev <danail.dev@gmail.com>",
  "scripts": {
    "test": "npm run test:ci",
    "test:dev": "jest --coverage",
    "test:ci": "jest --coverage --ci --runInBand",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "update": "npx -p npm-check-updates ncu -u && npm install --force",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "gatsby": "3.5.1",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "dependencies": {
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.1.9",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-preset-gatsby": "1.5.0",
    "babel-preset-react-app": "10.0.0",
    "fs-extra": "10.0.0",
    "gatsby": "3.5.1",
    "gatsby-plugin-utils": "1.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-image-snapshot": "4.5.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-test-renderer": "18.1.0",
    "semantic-release": "17.4.3"
  }
}
